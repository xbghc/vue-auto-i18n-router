{
  "name": "vitepress-auto-i18n-router",
  "version": "0.1.0",
  "description": "Automatic i18n routing for VitePress static sites",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./vitepress": {
      "import": "./src/vitepress/index.ts",
      "types": "./src/vitepress/index.ts"
    },
    "./components": {
      "import": "./src/components/index.ts",
      "types": "./src/components/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "vitepress",
    "i18n",
    "router",
    "internationalization",
    "vue",
    "vite-plugin",
    "auto-routing",
    "multilingual",
    "language-detection"
  ],
  "author": {
    "name": "xbghc",
    "email": "ghm20191606@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xbghc/vue-auto-i18n-router.git",
    "directory": "packages/i18n-router"
  },
  "bugs": {
    "url": "https://github.com/xbghc/vue-auto-i18n-router/issues"
  },
  "homepage": "https://github.com/xbghc/vue-auto-i18n-router/tree/main/packages/i18n-router#readme",
  "license": "MIT",
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "vitepress": "^1.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vitepress": "^1.6.4",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.4"
  }
}